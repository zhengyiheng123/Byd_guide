        android:layout_height="wrap_content">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="全部订单"
            android:padding="12dp"
            />
        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/line_color"
            android:layout_marginLeft="12dp"
            android:layout_marginRight="12dp"
            />
        <LinearLayout
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/tv_my_demand"
                android:drawablePadding="8dp"
            android:gravity="center"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="我的需求"
            android:drawableTop="@mipmap/ic_my_order"
            />
            <TextView
                android:id="@+id/tv_history_copy"
                android:drawablePadding="8dp"
                android:gravity="center"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="历史样本"
                android:drawableTop="@mipmap/ic_history"
                />
            <TextView
                android:id="@+id/tv_mygeted_orders"
                android:drawablePadding="8dp"
                android:gravity="center"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="我接的单"
                android:drawableTop="@mipmap/ic_received_order"
                />
            <TextView
                android:id="@+id/tv_myorder"
                android:drawablePadding="8dp"
                android:gravity="center"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:text="我的预定"
                android:drawableTop="@mipmap/ic_my_order"
                />
        </LinearLayout>
    </LinearLayout>
